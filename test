echo "a" | ./rgrep 'a'
echo "a" | ./rgrep 'b'
echo "b" | ./rgrep '.'
echo "b" | ./rgrep '..'
echo "abc" | ./rgrep '.b.'
echo "." | ./rgrep '\.'
echo "1" | ./rgrep '\.'
echo "+" | ./rgrep '\+'
echo "2" | ./rgrep '\+'
echo "?" | ./rgrep '\?'
echo "3" | ./rgrep '\?'
echo "\" | ./rgrep '\\'
echo "4" | ./rgrep '\\'
