echo "a" | ./rgrep 'a'
echo "a" | ./rgrep 'b'
echo "b" | ./rgrep '.'
echo "b" | ./rgrep '..'
echo "abc" | ./rgrep '.b.'
echo "." | ./rgrep '\.'
echo "1" | ./rgrep '\.'
echo "+" | ./rgrep '\+'
echo "2" | ./rgrep '\+'
echo "?" | ./rgrep '\?'
echo "3" | ./rgrep '\?'
echo "\\" | ./rgrep '\\'
echo "4" | ./rgrep '\\'
echo "0" | ./rgrep '1+'
echo "1" | ./rgrep '1+'
echo "22" | ./rgrep '2+'
echo "333" | ./rgrep '33?3'
echo "4444" | ./rgrep '444?44'
